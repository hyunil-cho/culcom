# 환경 설정 (.env.dev, .env.staging, .env.prod 중 하나를 .env로 복사하거나)
# APP_ENV 환경변수로 직접 지정
APP_ENV=dev

# 데이터베이스 설정
# 사용할 드라이버: mysql, postgres, sqlite3, sqlserver
DB_DRIVER=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=backoffice
DB_SSLMODE=disable

# 서버 설정
SERVER_PORT=8080
LOG_LEVEL=info
DEBUG=false
